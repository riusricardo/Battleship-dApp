    "geth-dev:init": "geth --datadir=.dev/datadir init .dev/datadir/gethGenesisBlock.json",
    "geth-dev:priv-chain": " geth --networkid=1337 --datadir=.dev/datadir --unlock 0b774fa1bcdcefb52b490f6879be8f8da0dbd954,417ed433e325a5e9d1f76396af5a4268b92a6bf1,47837f58b407550a4dfe4308f5e52c4d8f8f748e,c7dfca9d08ea64d4c40afc7c872ac268d68e8780,82294465b79cda4f493d7adccb504fce327d823d,2a61115c69a083d34f9da75f498e4733401bf280 --password config/dev_pass.txt --cache=1024 --cache.database=75 --port=30303 --rpc --rpcport=8545 --rpcaddr=localhost  --rpccorsdomain=\"*\" --nodiscover --maxpeers=0 --ws --wsport=8546 --wsaddr=localhost --wsorigins=\"*\" --mine --minerthreads=1 --etherbase=0x0b774fa1bcdcefb52b490f6879be8f8da0dbd954  --shh --rpcapi=eth,web3,net,debug,shh,personal --wsapi=eth,web3,net,shh,debug,personal --gpopercentile=60 --gpoblocks=20 --targetgaslimit=800000000 --gasprice=1000000000 --ethash.dagdir=.dev/datadir/geth/ --ethash.cachedir=.dev/datadir/geth/ --ethash.dagsinmem=1 --ethash.dagsondisk=1",
